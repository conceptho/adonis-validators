{
  "name": "@conceptho/adonis-validators",
  "version": "0.0.11",
  "deprecated": false,
  "description": "",
  "main": "index.js",
  "author": "Conceptho",
  "license": "MIT",
  "homepage": "https://github.com/conceptho/adonis-validators#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conceptho/adonis-validators.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "conceptho",
    "adonisjs",
    "validators"
  ],
  "scripts": {
    "eslint:check": "eslint ./",
    "eslint:fix": "prettier-eslint \"./**/*js\" --write",
    "pre-publish": "npm run eslint:check && npm publish",
    "test": "node japaFile.js",
    "coverage:test": "nyc npm test",
    "coverage:publish": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "bugs": {
    "url": "https://github.com/conceptho/adonis-validators/issues"
  },
  "devDependencies": {
    "codecov": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^1.3.1",
    "japa": "^2.0.7",
    "lint-staged": "^13.1.0",
    "nyc": "^13.3.0",
    "prettier-eslint-cli": "^4.7.1"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/validators/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ],
    "reporters": [
      "lcov"
    ]
  }
}
